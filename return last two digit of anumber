import java.util.*;

class UserMainCode
{
     public int secondLastDigitOf(int input1){
       if(input1>=0&&input1<10)||(input1<=0&&input1>-10)){
           return -1;
        }
       else if(input>0){
            int a=input1/10%10;
            return a;
        }
         else{
                int b=input1/10%10*-1;
                 return b;
           }

         }
